.data
.text
.globl main

main: 
li $s0, 3
move $a0, $s0
jal Fatorial
move $s1, $v0

Fatorial: move $t0, $a0 # Paasagem de parametro do n (vari√°vel local)
li $t1, 1 # result
li $t4, 1 # Flag

Loop: 
slt $t3, $t4, $t0 # n < 1
beq $t3, $0, Baixo # n >= 1

mul $t1, $t1, $t0
subi $t0, $t0, 1
j Loop

Baixo: move $v0, $t1 # Return