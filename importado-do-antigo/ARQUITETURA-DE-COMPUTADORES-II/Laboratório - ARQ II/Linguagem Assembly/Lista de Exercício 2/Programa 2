# Programa 2

.text #delimitar que será porção de códgo 

.globl main #indica aonde o programa começa

main: #rótulo ou label

#inicio

# Linguagem C
# x=1;
addi $s0, $0, 1

# y=5-x+15;
addi $s1, $0, 5

sub $t0, $s1, $s0 # 5 - 1 = 4 (a ordem irá alterar o resultado final)

addi $s2, $0, 15

add $s3, $t0, $s2 # y = 4 + 15 = 19
